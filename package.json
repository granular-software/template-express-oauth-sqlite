{
	"name": "joshu",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"dev:os": "turbo dev --filter=awmt-broker",
		"dev:apps": "turbo dev --filter=!awmt-broker",
		"gen": "turbo gen",
		"gen:component": "turbo gen component",
		"gen:model": "turbo gen model",
		"gen:interface": "turbo gen interface",
		"gen:list": "turbo gen list",
		"gen:publish": "turbo gen publish",
		"publish": "turbo gen publish",
		"push:mcpresso": "git subtree push --prefix=packages/mcpresso git@github.com:granular-software/mcpresso.git main",
		"push:mcpresso-oauth-server": "git subtree push --prefix=packages/mcpresso-oauth-server git@github.com:granular-software/mcpresso-oauth-server.git main",
		"push:mcpresso-openapi-generator": "git subtree push --prefix=packages/mcpresso-openapi-generator git@github.com:granular-software/mcpresso-openapi-generator.git main",
		"pull:mcpresso": "git subtree pull --prefix=packages/mcpresso git@github.com:granular-software/mcpresso.git main --squash",
		"pull:mcpresso-oauth-server": "git subtree pull --prefix=packages/mcpresso-oauth-server git@github.com:granular-software/mcpresso-oauth-server.git main --squash",
		"pull:mcpresso-openapi-generator": "git subtree pull --prefix=packages/mcpresso-openapi-generator git@github.com:granular-software/mcpresso-openapi-generator.git main --squash",
		"check:subtrees": "node scripts/check-subtree-status.js"
	},
	"devDependencies": {
		"prettier": "^3.5.3",
		"turbo": "^2.4.4",
		"typescript": "5.8.2"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.0.29",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"@radix-ui/react-checkbox": "^1.1.5",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-toggle": "^1.1.3",
		"@tailwindcss/postcss": "^4.1.10",
		"ai": "^4.3.16",
		"bufferutil": "^4.0.9",
		"jsonrepair": "^3.12.0",
		"next": "^15.2.4",
		"next-themes": "0.3.0",
		"postcss": "^8.5.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-rnd": "^10.5.2",
		"tailwindcss": "^4.1.10",
		"tiktoken": "^1.0.20",
		"utf-8-validate": "^6.0.5"
	}
}
